/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "paths": {
      "@env/*": ["src/environments/*"]
    },
    "strict": true,
    "sourceMap": true,
    "declaration": false,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "target": "es2020",
    "module": "es2020",
    "types": [
      "vite/client"
    ],
    "lib": [
      "es2018",
      "dom"
    ]
  }
}